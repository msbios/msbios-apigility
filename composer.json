{
  "name": "msbios/APIGILITY",
  "description": "MSBios I18n",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-apigility",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Apigility\\": "src/"
    },
    "files": [
      "constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Apigility\\": "tests/"
    }
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "msbios/msbios": "^1.0",
    "msbios/filter": "^1.0",
    "ccampbell/chromephp": "^4.1"
  },
  "require-dev": {
    "zfcampus/zf-apigility": "^1.3",
    "zfcampus/zf-apigility-documentation": "^1.2.3",
    "zfcampus/zf-development-mode": "^3.0",
    "zfr/zfr-cors": "^1.3",
    "zendframework/zend-cache": "^2.7.1",
    "zendframework/zend-mvc-i18n": "^1.0",
    "zendframework/zend-log": "^2.9",
    "zfcampus/zf-apigility-documentation-swagger": "^1.2",
    "msbios/test": "^1.0",
    "msbios/debug": "^1.0",
    "zfcampus/zf-apigility-admin": "^1.5.9",
    "zfcampus/zf-asset-manager": "^1.0",
    "zfcampus/zf-composer-autoloading": "^1.0",
    "zfcampus/zf-deploy": "^1.2"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push",
    "msgfmt": "find . -name \\*.po -execdir sh -c 'msgfmt \"$0\" -o `basename $0 .po`.mo' '{}' \\;",
    "serve": "php -S 0.0.0.0:3107 -t public index.php"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Apigility"
    }
  }
}
